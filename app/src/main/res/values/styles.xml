<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="roundImageView_profile_profileBanner_userIcon" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
        <item name="color">@color/white</item>
    </style>

    <style name="roundImageView_profile_myLiked_userIcon" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">35dp</item>
        <item name="color">@color/white</item>
    </style>

    <style name="roundImageView_profile_likedResto" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="android:topLeftRadius">100dp</item>
        <item name="android:topRightRadius">100dp</item>
        <item name="android:bottomRightRadius">0.01dp</item>
        <item name="android:bottomLeftRadius">0.01dp</item>
    </style>

    <style name="SpinnerStyle" parent="Widget.AppCompat.Spinner">
        <item name="android:background">@drawable/rounded_dropdown_option</item>
        <item name="android:gravity">center_vertical|end</item>
        <item name="android:spinnerMode">dropdown</item>
        <item name="android:editTextColor">@android:color/white</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">#535353</item>
        <item name="colorControlActivated">#FFC922</item>
    </style>

</resources>